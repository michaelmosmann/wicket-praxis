<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>de.wicketpraxis-quickstarts-15-seo</display-name>

	<!--
		There are three means to configure Wickets configuration mode and they 
		are tested in the order given.
		
		1) A system property: -Dwicket.configuration 
		2) servlet specific <init-param> 
		3) context specific <context-param>

		The value might be either "development" (reloading when templates change) or 
		"deployment". If no configuration is found, "development" is the default. -->

	<filter>
		<filter-name>wicket.de.wicketpraxis-quickstarts-15-seo</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
			<param-name>applicationClassName</param-name>
			<param-value>de.wicketpraxis.WicketApplication</param-value>
		</init-param>
	</filter>

  <filter>
	  <filter-name>de.wicketpraxis.sessionurl</filter-name>
  	<filter-class>de.wicketpraxis.DisabledJSessionIDinUrlFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>de.wicketpraxis.sessionurl</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
	<filter-mapping>
		<filter-name>wicket.de.wicketpraxis-quickstarts-15-seo</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
